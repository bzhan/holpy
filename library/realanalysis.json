             "vars": {
                 "f": "'a => real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_within_le",
             "prop": ["tendsto_real f l (within (at a) s) =",
                      "!e. 0 < e",
                           "--> ?d. 0 < d &",
                                "!x. member x s & 0 < abs (x - a) & abs (x - a) <= d",
                                "--> abs (f x - l) < e"],
             "ty": "thm",
             "vars": {
                 "f": "real => real",
                 "l": "real",
                 "a": "real",
                 "s": "real set"
             }
         },
         {
            "name": "reallim_within",
             "prop": ["tendsto_real f l (within (at a) s) =",
                      "!e. 0 < e",
                           "--> ?d. 0 < d &",
                                "!x. member x s & 0 < abs (x - a) & abs (x - a) < d",
                                "--> abs (f x - l) < e"],
             "ty": "thm",
             "vars": {
                 "f": "real => real",
                 "l": "real",
                 "a": "real",
                 "s": "real set"
             }
         },
         {
            "name": "reallim_at",
             "prop": ["tendsto_real f l (at a) =",
                      "!e. 0 < e",
                           "--> ?d. 0 < d & !x. 0 < abs (x - a) & abs (x - a) < d",
                                "--> abs (f x - l) < e"],
             "ty": "thm",
             "vars": {
                 "f": "real => real",
                 "l": "real",
                 "a": "real"
             }
         },
         {
            "name": "reallim_at_infinity",
             "prop": ["tendsto_real f l at_infinity =",
                      "!e. 0 < e --> ?b. !x. abs x >= b --> abs (f x - l) < e"],
             "ty": "thm",
             "vars": {
                 "f": "real => real",
                 "l": "real"
             }
         },
         {
            "name": "reallim_sequentially",
             "prop": ["tendsto_real s l sequentially =",
                      "!e. 0 < e --> ?N. !n. N <= n --> abs (s n - l) < e"],
             "ty": "thm",
             "vars": {
                 "s": "nat => real",
                 "l": "real"
             }
         },
         {
            "name": "reallim_eventually",
             "prop": "eventually (%x. f x = l) net --> tendsto_real f l net",
             "ty": "thm",
             "vars": {
                "net": "'a net",
                 "f": "'a => real",
                 "l": "real"
             }
         },
         {
            "name": "reallim_ubound",
             "prop": ["tendsto_real f l net &",
                      "neg (trivial_limit net) &",
                      "eventually (%x. f x <= b) net",
                      "--> l <= b"],
             "ty": "thm",
             "vars": {
                 "net": "'a net",
                 "f": "'a => real",
                 "l": "real",
                 "b": "real"
             }
         },
         {
            "name": "reallim_lbound",
             "prop": ["tendsto_real f l net &",
                      "neg (trivial_limit net) &",
                      "eventually (%x. b <= f x) net",
                      "--> b <= l"],
             "ty": "thm",
             "vars": {
                 "net": "'a net",
                 "f": "'a => real",
                 "l": "real",
                 "b": "real"
             }
         },
         {
            "name": "reallim_le",
             "prop": ["tendsto_real f l net & tendsto_real g m net &",
                      "neg (trivial_limit net) &",
                      "eventually (%x. f x <= g x) net",
                      "--> l <= m"],
             "ty": "thm",
             "vars": {
                 "net": "'a net",
                 "f": "'a => real",
                 "g": "'a => real",
                 "l": "real",
                 "m": "real"
             }
         },
         {
            "name": "reallim_const_eq",
             "prop": "tendsto_real (%x. c) d net = trivial_limit net | c = d",
             "ty": "thm",
             "vars": {
                 "net": "'a net",
                 "c": "real",
                 "d": "real"
             }
         },
         {
            "name": "reallim_sum",
             "prop": ["finite s & (!i. member i s --> tendsto_real (f i) (l i) net)",
                      "--> tendsto_real (%x. sum s (%i. f i x)) (sum s l) net"],
             "ty": "thm",
             "vars": {
                 "f": "'a => 'b => real",
                 "l": "'a => real",
                 "s": "'a set",
                 "net": "'b net"
             }
         },
         {
            "name": "reallim_null_sum",
             "prop": ["finite s & (!a. member a s --> tendsto_real (%x. f x a) 0 net)",
                      "--> tendsto_real (%x. sum s (f x)) 0 net"],
             "ty": "thm",
             "vars": {
                 "f": "'a => 'b => real",
                 "s": "'b set",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_null_comparison",
             "prop": ["eventually (%x. abs (f x) <= g x) net & tendsto_real g 0 net",
                      "--> tendsto_real f 0 net"],
             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "g": "'a => real",
                 "net": "'a net"
             }
         },
         {
            "name": "convergent_real_bounded_monotone",
             "prop": ["real_bounded (image s univ) &",
                      "((!n. s n <= s (Suc n)) | (!n. s (Suc n) <= s n))",
                      "--> ?l. tendsto_real s l sequentially"],
             "ty": "thm",
             "vars": {
                 "s": "nat => real"
             }
         },
         {
            "name": "reallim_eventually_ubound",
             "prop": "tendsto_real f l net & l < c --> eventually (%x. f x < c) net",
             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "l": "real",
                 "c": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_eventually_lbound",
             "prop": "tendsto_real f l net & c < l --> eventually (%x. c < f x) net",
             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "l": "real",
                 "c": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_sequentially_within",
             "prop": "tendsto_real f l sequentially --> tendsto_real f l (within sequentially s)",
             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "l": "real",
                 "s": "nat set"
             }
         },
         {
             "name": "real_sums",
             "prop": "real_sums f l s = tendsto_real (%n. sum (inter s {0..n}) f) l sequentially",
             "ty": "def",
             "type": "(nat => real) => real => nat set => bool"  
         },
         {
            "name": "real_infsum",
            "prop": "real_infsum s f = THE l. real_sums f l s",
            "ty": "def",
            "type": "nat set => (nat => real) => real"  
        },
        {
            "name": "real_summable",
            "prop": "real_summable s f = ?l. real_sums f l s",
            "ty": "def",
            "type": "nat set => (nat => real) => bool"  
        },
        {
            "name": "real_series_cauchy",
             "prop": ["(?l. real_sums f l s) =",
                      "!e. 0 < e",
                           "--> ?N. !m. !n. m >= N --> abs (sum (inter s {m..n}) f) < e"],
             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "s": "nat set"
             }
         },
         {
            "name": "real_summable_cauchy",
             "prop": ["real_summable s f =",
                      "!e. 0 < e",
                           "--> ?N. !m. !n. m >= N --> abs (sum (inter s {m..n}) f) < e"],
             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "s": "nat set"
             }
         },
         {
            "name": "real_sums_summable",
             "prop": "real_sums f l s --> real_summable s f",
             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "l": "real",
                 "s": "nat set"
             }
         },
         {
            "name": "real_sums_infsum",
             "prop": "real_sums f (real_infsum s f) s = real_summable s f",
             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "s": "nat set"
             }
         },
         {
             "name": "from",
             "prop": "from n = {m. n <= m}",
             "ty": "def",
             "type": "nat => nat set"             
         },
         {
            "name": "real_series_from",
             "prop": "real_sums f l (from k) = tendsto_real (%n. sum {k..n} f) l sequentially",
             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "l": "real",
                 "k": "nat"
             }
         },
         {
            "name": "real_series_unique",
             "prop": "real_sums f l1 s & real_sums f l2 s --> l1 = l2",
             "ty": "thm",

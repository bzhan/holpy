                 "l": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_lmul_eq",
             "prop": "neg (c = 0) --> tendsto_real (%x. c * f x) (c * l) net = tendsto_real f l net",
             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "c": "real",
                 "l": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_rmul_eq",
             "prop": "neg (c = 0) --> tendsto_real (%x. (f x) * c) (l * c) net = tendsto_real f l net",
             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "c": "real",
                 "l": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_neg_eq",
             "prop": "tendsto_real (%x. -(f x)) (-l) net = tendsto_real f l net ",

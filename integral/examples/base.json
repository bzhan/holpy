{
    "content": [
        {
            "type": "header",
            "name": "Common integrals",
            "level": 1
        },
        {
            "type": "axiom",
            "expr": "(INT x. c) = c * x + C"
        },
        {
            "type": "axiom",
            "expr": "(INT x. x ^ (-1)) = log(abs(x)) + C"
        },
        {
            "type": "axiom",
            "expr": "(INT x. x ^ n) = x ^ (n + 1) / (n + 1) + C",
            "conds": [
                "n != -1"
            ]
        },
        {
            "type": "axiom",
            "expr": "(INT x. exp(x)) = exp(x) + C"
        },
        {
            "type": "axiom",
            "expr": "(INT x. sin(x)) = -cos(x) + C"
        },
        {
            "type": "axiom",
            "expr": "(INT x. cos(x)) = sin(x) + C"
        },
        {
            "type": "axiom",
            "expr": "(INT x. 1 / (x^2 + 1)) = atan(x) + C"
        },
        {
            "type": "axiom",
            "expr": "(INT x. sec(x)^2) = tan(x) + C"
        },
        {
            "type": "axiom",
            "expr": "(INT x. csc(x)^2) = -cot(x) + C"
        },
        {
            "type": "header",
            "name": "Common series expansion",
            "level": 1
        },
        {
            "type": "axiom",
            "expr": "exp(x) = SUM(n, 0, oo, x ^ n / factorial(n))"
        },
        {
            "type": "axiom",
            "expr": "sin(x) = SUM(n, 0, oo, (-1)^n * x^(2*n+1) / factorial(2*n+1))"
        },
        {
            "type": "axiom",
            "expr": "cos(x) = SUM(n, 0, oo, (-1)^n * x^(2*n) / factorial(2*n))"
        },
        {
            "type": "axiom",
            "expr": "atan(x) = SUM(n, 0, oo, (-1)^n * x^(2*n+1) / (2*n+1))",
            "conds": [
                "abs(x) <= 1"
            ]
        },
        {
            "type": "axiom",
            "expr": "(1 + x) ^ -1 = SUM(n, 0, oo, (-1)^n * x ^ n)",
            "conds": [
                "abs(x) < 1"
            ]
        },
        {
            "type": "axiom",
            "expr": "log(1 + x) = SUM(n, 0, oo, (-1)^n * x ^ (n+1) / (n + 1))",
            "conds": [
                "x > -1",
                "x <= 1"
            ]
        },
        {
            "type": "axiom",
            "expr": "log(1 - x) = SUM(n, 0, oo, (-1)^n * (-x)^(n+1) / (n + 1))",
            "conds": [
                "x >= -1",
                "x < 1"
            ]
        },
        {
            "type": "header",
            "name": "Common series evaluations",
            "level": 1
        },
        {
            "type": "axiom",
            "expr": "SUM(n, 0, oo, (n+1)^(-2)) = (pi^2) / 6"
        },
        {
            "type": "axiom",
            "expr": "SUM(n, 0, oo, (-1)^n * (n+1)^(-2)) = (pi^2) / 12"
        },
        {
            "type": "header",
            "name": "Hyperbolic functions",
            "level": 1
        },
        {
            "type": "definition",
            "expr": "cosh(x) = (exp(x) + exp(-x)) / 2"
        },
        {
            "type": "header",
            "name": "Exp-log identities",
            "level": 1
        },
        {
            "type": "axiom",
            "expr": "exp(a + b) = exp(a) * exp(b)",
            "category": "exp-log"
        },
        {
            "type": "axiom",
            "expr": "exp(a - b) = exp(a) * exp(-b)",
            "category": "exp-log"
        },
        {
            "type": "axiom",
            "expr": "log(a * b) = log(a) + log(b)",
            "category": "exp-log"
        },
        {
            "type": "axiom",
            "expr": "log(a / b) = log(a) - log(b)",
            "category": "exp-log"
        },
        {
            "type": "header",
            "name": "Trigonometric identities",
            "level": 1
        },
        {
            "type": "axiom",
            "expr": "atan(x ^ -1) = pi/2 - atan(x)",
            "category": "trigonometric"
        },
        {
            "type": "axiom",
            "expr": "sin(acos(x)) = sqrt(1-x^2)",
            "category": "trigonometric"
        },
        {
            "type": "header",
            "name": "Factorial and binomial coefficient",
            "level": 1
        },
        {
            "type": "axiom",
            "expr": "(m + 1) * factorial(m) = factorial(m + 1)",
            "category": "factorial"
        },
        {
            "type": "axiom",
            "expr": "m * factorial(m - 1) = factorial(m)",
            "category": "factorial"
        },
        {
            "type": "axiom",
            "expr": "binom(2*k+2, k+1) = 2 * binom(2*k, k) * ((2*k+1) / (k+1))",
            "category": "binomial"
        },
        {
            "type": "header",
            "name": "Derived integrals",
            "level": 1
        },
        {
            "type": "problem",
            "expr": "(INT x. 1 / (x + a)) = log(abs(x + a)) + C",
            "path": "standard"
        },
        {
            "type": "problem",
            "expr": "(INT x. exp(a * x)) = a ^ (-1) * exp(a * x) + C",
            "path": "standard"
        },
        {
            "type": "problem",
            "expr": "(INT x. sin(a * x)) = - (a ^ (-1) * cos(a * x)) + C",
            "path": "standard"
        },
        {
            "type": "problem",
            "expr": "(INT x. cos(a * x)) = a ^ (-1) * sin(a * x) + C",
            "path": "standard"
        },
        {
            "type": "problem",
            "expr": "(INT x. 1 / (a ^ 2 + x ^ 2)) = a ^ (-1) * atan(x / a) + C",
            "conds": [
                "a != 0"
            ],
            "path": "standard"
        },
        {
            "type": "problem",
            "expr": "(INT x. x ^ k * log(x)) = x ^ (k + 1) * log(x) * (k + 1) ^ (-1) - x ^ (k + 1) * (k + 1) ^ (-2) + SKOLEM_CONST(C)",
            "path": "standard"
        },
        {
            "type": "problem",
            "expr": "(INT x:[0,1]. x ^ m * log(x) ^ n) = (-1)^n * factorial(n) / (m+1) ^ (n+1)",
            "path": "standard"
        }
    ],
    "name": "Base theory"
}
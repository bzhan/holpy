            "vars": {
                "s": "real set"
            }
        },
        {
            "attributes": [
                "hint_rewrite"
            ],
            "name": "tendsto_real",
            "prop": "tendsto_real f l net = !e. 0 < e --> eventualy (%x. abs (f x - l) < e) net",
            "ty": "def",
            "type": "('a => real) => real => 'a net => bool"
        },
        {
            "attributes": [
                "hint_rewrite"
            ],
            "name": "reallim",
            "prop": "reallim net f = THE l. tendsto_real f l net",
            "ty": "def",
            "type": "'a net => ('a => real) => real"   
        },
        {
            "attributes": [
                "hint_rewrite"
            ],
            "name": "trivial_limit",
            "prop": "trivial_limit net = eventualy (%x::'a. false) net",
            "ty": "def",
            "type": "'a net => bool"   
        },
        {
            "name": "reallim_trivial",
            "prop": "trivial_limit net --> tendsto_real f l net",
            "ty": "thm",
            "vars": {
                "f": "'a => real",
                "l": "real",
                "net": "'a net"
            }
        },
        {
           "name": "reallim_unique",
            "prop": "neg (trivial_limit net) & tendsto_real f l1 net & tendsto_real f l2 net --> l1 = l2",
            "ty": "thm",
            "vars": {
                "f": "'a => real",
                "l1": "real",
                "l2": "real",
                "net": "'a net"
            }
        },
        {
            "name": "reallim_const",
             "prop": "tendsto_real (%x. a) a net",
             "ty": "thm",
             "vars": {
                 "a": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_lmul",
             "prop": "tendsto_real f l net --> tendsto_real (%x. c * f x) (c * l) net",
             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "c": "real",
                 "l": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_rmul",
             "prop": "tendsto_real f l net --> tendsto_real (%x. (f x) * c) (l * c) net",
             "ty": "thm",
             "vars": {
                 "f": "'a => real",

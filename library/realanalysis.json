             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "l": "real",
                 "k": "nat"
             }
         },
         {
            "name": "reallim_transform_straddle",
             "prop": ["eventually (%n. f n <= g n) net & tendsto_real f a net &",

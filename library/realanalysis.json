                      "--> tendsto_real g a net"],
             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "g": "'a => real",
                 "h": "'a => real",
                 "a": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_null_abs",
             "prop": "tendsto_real (%x. abs (f x)) 0 net = tendsto_real f 0 net",

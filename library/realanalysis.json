             "vars": {
                 "f": "'a => real",
                 "g": "'a => real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_null_lmul",
             "prop": "tendsto_real f 0 net --> tendsto_real (%x. c * f x) 0 net",

             "ty": "thm",
             "vars": {
                 "f": "'a => real",
                 "l": "real",
                 "g": "'a => real",
                 "m": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_abs",
             "prop": "tendsto_real f l net --> tendsto_real (%x. abs (f x)) (abs l) net",

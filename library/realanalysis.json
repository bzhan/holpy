             "vars": {
                 "f": "'a => real",
                 "g": "'a => real",
                 "l": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "real_seq_offset",
             "prop": ["tendsto_real f l sequentially",
                      "--> tendsto_real (%i. f (i + k)) l sequentially"],
             "ty": "thm",
             "vars": {
                 "f": "nat => real",
                 "l": "real",
                 "k": "nat"
             }
         },
         {
            "name": "real_seq_offset_rev",
             "prop": ["tendsto_real (%i. f (i + k)) l sequentially",

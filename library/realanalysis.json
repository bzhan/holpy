             "vars": {
                 "f": "'a => real",
                 "g": "'a => real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_transform_eventually",
             "prop": ["eventually (%x. f x = g x) net & tendsto_real f l net",

             "vars": {
                 "f": "'a => real",
                 "l": "real",
                 "g": "'a => real",
                 "m": "real",
                 "net": "'a net"
             }
         },
         {
            "name": "reallim_min",
             "prop": ["tendsto_real f l net & tendsto_real g m net",
